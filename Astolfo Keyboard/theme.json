{
    "name": "Astolfo Keyboard",
    "version": "v5.1",
    "author": "Party Wumpus",
    "target": "Keyboard",
    "manifest_version": 2,
  "description": "A simple keyboard theme based off the character Astolfo.",
    "inject": {
        "shared.css": [
            "SP"
        ] 
    },
    "patches": {
        "Black Letters": {
            "default": "No",
            "type": "checkbox",
            "values": {
                "No": {},
                "Yes": {"Letter Colours/black.css": ["SP"]}
            }
        }
    }
}
